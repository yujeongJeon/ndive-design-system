{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node.js",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ESNext",
        "strict": true,
        "outDir": "dist",
        "moduleResolution": "Bundler",
        "module": "ESNext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "allowJs": true,
        "resolveJsonModule": true
    }
}
