{
    "name": "@ndive/design-components",
    "version": "0.0.6",
    "description": "",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "fetch:color": "node ./scripts/fetchColor.js",
        "fetch:typo": "node ./scripts/fetchTypo.js",
        "fetch:icon": "node ./scripts/fetchIcon.js",
        "clean": "rm -rf dist",
        "build": "vite build --config vite.config.mts"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./icons": {
            "import": "./dist/icons.mjs",
            "types": "./dist/icons.d.ts"
        },
        "./styles": {
            "default": "./dist/style.css"
        },
        "./color": {
            "default": "./dist/json/color.json"
        },
        "./icon": {
            "default": "./dist/json/icon.json"
        },
        "./size": {
            "default": "./dist/json/size.json"
        },
        "./scss/typo.scss": {
            "default": "./dist/styles/typo.scss"
        }
    },
    "keywords": [],
    "author": "yujeong-jeon",
    "license": "ISC",
    "devDependencies": {
        "@ndive/design-tokens": "workspace:^",
        "@ndive/tsconfig": "workspace:*",
        "@ndive/vite": "workspace:^",
        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@svgr/plugin-prettier": "^8.1.0",
        "@svgr/plugin-svgo": "^8.1.0",
        "@types/node": "^22.5.5",
        "@types/react": "^18.3.9",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.2",
        "browserslist-to-esbuild": "^2.1.1",
        "dotenv": "^16.4.5",
        "react": "^18",
        "react-dom": "^18",
        "sass": "^1.79.4",
        "vite": "^5.4.6",
        "vite-plugin-static-copy": "^2.0.0"
    },
    "peerDependencies": {
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "react": "^18",
        "react-dom": "^18"
    },
    "dependencies": {
        "@fontsource/pretendard": "^5.1.0",
        "classnames": "^2.5.1"
    }
}
