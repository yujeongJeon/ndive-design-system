{
    "$schema": "https://json.schemastore.org/package",
    "name": "@ndive/design-tokens",
    "version": "0.0.7",
    "description": "Package containing Node.js module to extract foundation from ndive-design-system figma",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "files": [
        "dist"
    ],
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        }
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "vite build --config vite.config.ts"
    },
    "keywords": [
        "ndive",
        "design-system",
        "tokens"
    ],
    "author": {
        "name": "yujeong-jeon",
        "email": "workingnewjeong@gmail.com"
    },
    "contributors": [
        {
            "email": "workingnewjeong@gmail.com",
            "name": "yujeongJeon"
        },
        {
            "email": "root@yceffort.kr",
            "name": "yceffort"
        }
    ],
    "license": "MIT",
    "devDependencies": {
        "@ndive/tsconfig": "workspace:*",
        "@ndive/vite": "workspace:^",
        "@types/node": "^22.5.5",
        "@types/qs": "^6.9.16",
        "vite": "^5.4.6"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "qs": "^6.13.0"
    }
}
