{
    "$schema": "https://json.schemastore.org/package",
    "name": "@ndive/design-tracker",
    "version": "0.0.7",
    "description": "This CLI tool provides a static analysis of @ndive/design-components usage in your codebase.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": "./dist/index.js",
    "files": [
        "dist"
    ],
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsup bin/index.ts",
        "clean": "rm -rf dist"
    },
    "keywords": ["ndive", "design-system", "tracker"],
    "author": {
        "name": "yujeong-jeon",
        "email": "workingnewjeong@gmail.com"
    },
    "contributors": [
        {
            "email": "workingnewjeong@gmail.com",
            "name": "yujeongJeon"
        },
        {
            "email": "root@yceffort.kr",
            "name": "yceffort"
        }
    ],
    "license": "MIT",
    "devDependencies": {
        "@ndive/tsconfig": "workspace:*",
        "@types/node": "^22.5.5",
        "tsup": "^8.3.0"
    },
    "dependencies": {
        "cli-meow-help": "^4.0.0",
        "cosmiconfig": "^9.0.0",
        "meow": "^13.2.0",
        "minimatch": "^10.0.1",
        "ts-morph": "^24.0.0"
    }
}
